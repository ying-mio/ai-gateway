# 网关鉴权（给客户端用）
GATEWAY_TOKEN=replace_with_gateway_token

# 上游配置（网关转发时使用）
UPSTREAM_BASE_URL=https://openrouter.ai/api/v1
UPSTREAM_API_KEY=replace_with_upstream_key
UPSTREAM_TIMEOUT=60

# 默认模型（请求未指定 model 时兜底）
DEFAULT_MODEL=openai/gpt-4o-mini

# 可选：标记上游类型（openrouter/openai/other）
UPSTREAM_PROVIDER=openrouter

# 可选：仅 OpenRouter 推荐头
OPENROUTER_REFERER=https://foxrelay.xyz
OPENROUTER_TITLE=ai-gateway
